varnishtest "Minimal test template"

server s1 {
   rxreq
   txresp
} -repeat 1 -start

varnish v1 -vcl+backend {
    import redis from "${vmod_topbuild}/src/.libs/libvmod_redis.so";

    sub vcl_init {
        redis.init("main", "${redis_master1_address}", 500, 0, 0, 0, 0, false, 1);
    }
} -start

client c1 {
    txreq
    rxresp
} -run

varnish v1 -expect client_req == 1
