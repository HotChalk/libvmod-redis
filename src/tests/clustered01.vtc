varnishtest "Minimal test template"

server s1 {
   rxreq
   txresp
} -repeat 1 -start

varnish v1 -vcl+backend {
    import redis from "${vmod_topbuild}/src/.libs/libvmod_redis.so";

    sub vcl_init {
        new db = redis.db("cluster", "${redis_master1}", 500, 0, 0, 16, 0, true, 32);
        db.add_cserver("${redis_master2}");
        db.add_cserver("${redis_master3}");
    }

    sub vcl_fini {
        db.fini();
    }
} -start

client c1 {
    txreq
    rxresp
} -run

varnish v1 -expect client_req == 1
